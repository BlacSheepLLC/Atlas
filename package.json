{
  "name": "@blac-sheep/atlas",
  "version": "0.1.0",
  "description": "A simple library containing information about countries and their territories.",
  "repository": {
    "type": "git",
    "url": "https://github.com/BlacSheepLLC/Atlas.git"
  },
  "main": "dist/atlas.min.js",
  "module": "src/index.js",
  "author": "Blac-Sheep LLC",
  "bugs": {
    "url": "https://github.com/BlacSheepLLC/Atlas/issues"
  },
  "license": "GPL-3.0",
  "private": false,
  "scripts": {
    "serve": "parcel serve ./test/index.html --open",
    "build": "parcel build ./src/index.js --global Atlas --out-file atlas.js --no-minify --no-cache --public-url . && parcel build ./src/index.js --global Atlas --out-file atlas.min.js --no-cache --public-url .",
    "watch": "parcel watch ./src/index.js --global Atlas --out-file atlas.js --no-cache --public-url ."
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-object-rest-spread": "^7.2.0",
    "@babel/preset-env": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "core-js": "^3.2.1",
    "eslint": "^6.5.1",
    "parcel": "^1.12.3"
  },
  "files": [
	"package.json",
	"yarn.lock",
    "src/*",
	"dist/*",
	"LICENSE.md",
	"README.md"
  ]
}
